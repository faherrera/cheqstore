@model IEnumerable<CheqStore.Models.Rol>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Layouts/General/_Layout1.cshtml";
}

<h2>Los roles</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @if(Model.Count() <= 0){
                     @Html.Raw("No tengo datos aún");
                }else
                {

                    @item.Name
                }
            </td>
        </tr>
     }

</table>